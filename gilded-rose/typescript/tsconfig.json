{
	"compilerOptions": {
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		"strict": false,
		"noUncheckedIndexedAccess": true,
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"outDir": "dist",
		"sourceMap": true,
		"noEmit": true,
		"lib": ["es2022"]
	}
}
